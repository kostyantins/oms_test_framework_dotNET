<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
       assembly="oms_test_framework_dotNET"
       namespace="oms_test_framework_dotNET.Domains">

  <class name="OrderItem" table="OrderItems">
    <id name="Id">
      <column name="ID"/>
      <generator class="native"/>
    </id>
    <property name="Cost"/>
    <property name="ItemPrice"/>
    <property name="Quantity"/>
    <property name="DimensionRef"/>
    <property name="OrderRef"/>
    <property name="ProductRef"/>  

  </class>

</hibernate-mapping>